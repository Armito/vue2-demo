{
    "plugins": ["plugins/markdown"],
    "source": {
        "include": ["src/api"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "sourceType": "module",
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "template": "node_modules/docdash"
    }
}
