{
    "plugins": ["node_modules/jsdoc-vue"],
    "source": {
        "include": ["src/api", "src/components"],
        "includePattern": "\\.(vue|js)$",
        "excludePattern": "(node_modules/|docs)"
    },
    "sourceType": "module",
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/",
        "template": "node_modules/docdash"
    }
}
